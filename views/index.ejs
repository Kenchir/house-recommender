              <% houses.forEach((house,i)=>{%>
                
              
               <div class="card" style="width: 30rem;">
                <div class="card-body">
                  
                   <span class="card-title"></a></strong></span>  <span class="card-title"><strong> <a href="/houses/<%= house.name %>"><%= house.name %></a></strong><span class="float-sm-right "><i> <%= moment(house.uploadedAt).fromNow() %></i></span></5>
                  <p class="card-text"><%= house.details %></p>
                
                  
