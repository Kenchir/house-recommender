                <div class="box-footer">
                  <div id="footer-cont">
                    
                    
                    
                         
                         <button class="like-btn" data="<%=house._id%>" type="submit" value="<%= house.name %>">
                                  <span id="likes-num"><%= house.likes %></span>
                                  <span id='clickableAwesomeFont'>
                                   <i class="fas fa-thumbs-up fa-1x"></i><span> like</span>
                          </button>  
                          
                     <form  class="float-md-right">
                        <button class="share-btn">
                             <span id='clickableAwesomeFont'> 
                                <i class="fas fa-share-square fa-1x" ></i>
                             </span>
                        </button>
                    </form>
                   
                <div class="comment-box">
                  <span class="view"> View comments</span>
                    <div class="detailBox">
                        <div class="titleBox">
                          <label>Comments</label>
                            <button type="button" class="close" aria-hidden="true">&times;</button>
                        </div>
                       
                        <div class="actionBox">
                            <ul class="commentList">
                              <%comments.forEach((comment,j)=>{%>
                                    <% if(comment.house_id==house._id){%>
                                           <li>
                                                <% users.forEach((user,k)=>{%>
                                                     <% if(user._id==comment.user_id){%>
                                                        <h5><%= user.fname%> <%= user.lname%></h5>
                                                        <span class="pull-right"> <%= comment.createdAt%></span>
                                                      <% }%>
                                                <% })%>
                                                  <p class="comment-text"><%= comment.text%></p> <span class="date sub-text"></span>
                                          </li>
                                    
                                    <% }%>
                                <% })%>
                            </ul>
                            <div class="form-group comment-div">
                                <textarea class="form-control comment-cont" id="exampleFormControlTextarea1"  rows="2"></textarea>
                                <button class="btn btn-primary btn-sm comment-btn" data="<%= house._id%>" >Send</button>
                          </div>
                        </div>
                    </div>
                </div>
                    
                    
                  </div>
               </div>
               
                   </div>
                   
                </div>
              </div>
             <% }) %>
